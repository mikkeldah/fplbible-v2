 i = 1; i < topThree.length; i++) {\r\n        if (topThree[i][1] < res[1]) {\r\n            res = topThree[i]\r\n        }\r\n    }\r\n\r\n    return res;\r\n}\r\n\r\nfunction getTopAttributeScore(data, att) {\r\n    let topScore = 0;\r\n\r\n    for (const key in data) {\r\n        const player = data[key]\r\n        if (player[att] > topScore) {\r\n            topScore = player[att];\r\n        }\r\n    }\r\n\r\n    return topScore;\r\n}\r\n\r\nexport default Customized;","C:\\Users\\mikke\\VSC\\fantasy-django-react-new\\src\\components\\captain-picks-page\\sub-components\\GeneralCaptainPicks.js",["179","180","181"],"import { useEffect, useState } from 'react';\r\nimport CaptainPick from './CaptainPick';\r\n\r\nfunction General( props ) {\r\n\r\n    //Fetching playerdata\r\n    const [ playerData, setPlayerData ] = useState([])\r\n\r\n    useEffect(() => {\r\n        fetch(props.apiURL+'players')\r\n        .then(res => res.json())\r\n        .then(data => {\r\n            setPlayerData(data);\r\n        })\r\n        .catch(err => {\r\n            console.log(err);\r\n        });\r\n    }, []); \r\n\r\n    //Fetching fixturedata\r\n    const [ fixtureData, setFixtureData ] = useState([])\r\n\r\n    useEffect(() => {\r\n        fetch(props.apiURL+'fixtures')\r\n        .then(res => res.json())\r\n        .then(data => {\r\n            setFixtureData(data)\r\n        })\r\n        .catch(err => {\r\n            console.log(err);\r\n        });\r\n    }, []);\r\n\r\n    return (\r\n        <div className=\"captain-picks-container\">\r\n            <div id=\"captain-picks-general-title\">\r\n                <h2>Top three captain picks for GW{props.gameweek.id}</h2>\r\n            </div>\r\n            <div id=\"captain-picks-general-picks-container\">\r\n                {getTopThreeCaptainPicks(playerData, fixtureData, props.gameweek.id).map((instance, i) => {\r\n                    const [ player, cScore ] = instance;\r\n                    return (\r\n             